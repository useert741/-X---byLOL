    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const deltaUrlInput = document.getElementById('delta-url');
            const useCustomKeyCheckbox = document.getElementById('use-custom-key');
            const apiKeyContainer = document.getElementById('api-key-container');
            const apiKeyInput = document.getElementById('api-key');
            const bypassBtn = document.getElementById('bypass-btn');
            const loadingElement = document.getElementById('loading');
            const resultContainer = document.getElementById('result-container');
            const resultContent = document.getElementById('result-content');
            const copyBtn = document.getElementById('copy-btn');
            
            // é»˜è®¤APIå¯†é’¥ï¼ˆåœ¨å®é™…åº”ç”¨ä¸­åº”è¯¥ä»å®‰å…¨çš„åœ°æ–¹è·å–ï¼‰
            const defaultApiKey = 'DEMO_API_KEY';
            
            // åˆ‡æ¢è‡ªå®šä¹‰APIå¯†é’¥è¾“å…¥æ¡†
            useCustomKeyCheckbox.addEventListener('change', function() {
                if (this.checked) {
                    apiKeyContainer.style.display = 'block';
                } else {
                    apiKeyContainer.style.display = 'none';
                }
            });
            
            // è§£å¡æŒ‰é’®ç‚¹å‡»äº‹ä»¶
            bypassBtn.addEventListener('click', function() {
                const deltaUrl = deltaUrlInput.value.trim();
                
                if (!deltaUrl) {
                    alert('è¯·è¾“å…¥ Delta è®¤è¯é“¾æ¥');
                    return;
                }
                
                // æ£€æŸ¥é“¾æ¥æ˜¯å¦æœ‰æ•ˆ
                const deltaDomains = [
                    "https://auth.platoboost.com",
                    "https://auth.platorelay.com", 
                    "https://auth.platoboost.net",
                    "https://auth.platoboost.click"
                ];
                
                const isValidDeltaLink = deltaDomains.some(domain => deltaUrl.includes(domain));
                if (!isValidDeltaLink) {
                    alert('è¯·è¾“å…¥æœ‰æ•ˆçš„ Delta è®¤è¯é“¾æ¥');
                    return;
                }
                
                // è·å–APIå¯†é’¥
                const apiKey = useCustomKeyCheckbox.checked ? apiKeyInput.value.trim() : defaultApiKey;
                if (!apiKey) {
                    alert('è¯·è¾“å…¥ API å¯†é’¥');
                    return;
                }
                
                // æ˜¾ç¤ºåŠ è½½çŠ¶æ€
                loadingElement.style.display = 'block';
                resultContainer.style.display = 'none';
                bypassBtn.disabled = true;
                
                // æ¨¡æ‹ŸAPIè°ƒç”¨ï¼ˆåœ¨å®é™…åº”ç”¨ä¸­æ›¿æ¢ä¸ºçœŸå®çš„APIè°ƒç”¨ï¼‰
                simulateBypassAPI(deltaUrl, apiKey);
            });
            
            // å¤åˆ¶ç»“æœæŒ‰é’®
            copyBtn.addEventListener('click', function() {
                const resultText = resultContent.innerText;
                navigator.clipboard.writeText(resultText).then(() => {
                    const originalText = copyBtn.innerHTML;
                    copyBtn.innerHTML = '<i class="fas fa-check"></i> å·²å¤åˆ¶';
                    setTimeout(() => {
                        copyBtn.innerHTML = originalText;
                    }, 2000);
                });
            });
            
            // æ¨¡æ‹ŸAPIè°ƒç”¨å‡½æ•°
            function simulateBypassAPI(url, apiKey) {
                // åœ¨å®é™…åº”ç”¨ä¸­ï¼Œè¿™é‡Œåº”è¯¥è°ƒç”¨çœŸå®çš„API
                // ä¾‹å¦‚ï¼šfetch(`https://bypass.ceo/bypass/premium/delta?url=${encodeURIComponent(url)}&api_key=${apiKey}`)
                
                // æ¨¡æ‹Ÿç½‘ç»œå»¶è¿Ÿ
                setTimeout(() => {
                    // æ¨¡æ‹ŸæˆåŠŸå“åº”
                    const mockResponse = {
                        key: "DELTA-7X9B2-C8M3N-1P5Q6",
                        time: "1250",
                        error: null
                    };
                    
                    // æ¨¡æ‹Ÿé”™è¯¯å“åº”ï¼ˆå–æ¶ˆæ³¨é‡Šæµ‹è¯•é”™è¯¯æƒ…å†µï¼‰
                    // const mockResponse = {
                    //     key: null,
                    //     time: null,
                    //     error: "æ— æ•ˆçš„é“¾æ¥æˆ–APIå¯†é’¥"
                    // };
                    
                    displayResult(mockResponse);
                }, 2000);
            }
            
            // æ˜¾ç¤ºç»“æœå‡½æ•°
            function displayResult(data) {
                loadingElement.style.display = 'none';
                resultContainer.style.display = 'block';
                bypassBtn.disabled = false;
                
                let resultHTML = '';
                
                if (data.key) {
                    resultHTML += `
                        <div class="result-item">
                            <span class="result-label">ğŸ”‘ å¡å¯†:</span>
                            <span class="success">${data.key}</span>
                        </div>
                    `;
                } else {
                    resultHTML += `
                        <div class="result-item">
                            <span class="result-label">ğŸ”‘ å¡å¯†:</span>
                            <span class="error">è·å–å¤±è´¥</span>
                        </div>
                    `;
                }
                
                if (data.time) {
                    resultHTML += `
                        <div class="result-item">
                            <span class="result-label">â±ï¸ è€—æ—¶:</span>
                            <span>${data.time}ms</span>
                        </div>
                    `;
                }
                
                if (data.error) {
                    resultHTML += `
                        <div class="result-item">
                            <span class="result-label">âš ï¸ é”™è¯¯:</span>
                            <span class="error">${data.error}</span>
                        </div>
                    `;
                }
                
                resultHTML += `
                    <div class="result-item">
                        <span class="result-label">ğŸ’¡ æ¥æº:</span>
                        <span>LATstudio</span>
                    </div>
                `;
                
                resultContent.innerHTML = resultHTML;
            }
            
            // ç²˜è´´åŠŸèƒ½
            deltaUrlInput.addEventListener('click', function() {
                navigator.clipboard.readText().then(text => {
                    if (text.includes('auth.platoboost') || text.includes('auth.platorelay')) {
                        deltaUrlInput.value = text;
                    }
                }).catch(err => {
                    console.log('æ— æ³•è¯»å–å‰ªè´´æ¿:', err);
                });
            });
        });
    </script>
