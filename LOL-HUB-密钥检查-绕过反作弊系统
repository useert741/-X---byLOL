-- æ£€æµ‹æ¸¸æˆä¸­çš„åä½œå¼Šç³»ç»Ÿ
local function detectGameAntiCheat()
    local detectedAC = {}
    
    -- æ£€æµ‹å¸¸è§çš„åä½œå¼Šè„šæœ¬
    local suspiciousScripts = {
        "AntiCheat", "AC_", "_AC", "Security", "Guard", "Shield",
        "Protection", "AntiExploit", "AntiHack", "CheatDetection"
    }
    
    -- æ£€æŸ¥æ¸¸æˆä¸­çš„æ‰€æœ‰è„šæœ¬
    for _, obj in ipairs(game:GetDescendants()) do
        if obj:IsA("Script") or obj:IsA("LocalScript") or obj:IsA("ModuleScript") then
            local name = obj.Name
            local fullName = obj:GetFullName()
            
            for _, acName in ipairs(suspiciousScripts) do
                if string.find(name, acName) or string.find(fullName, acName) then
                    table.insert(detectedAC, {
                        Name = name,
                        Path = fullName,
                        Type = obj.ClassName
                    })
                    break
                end
            end
        end
    end
    
    return detectedAC
end

-- æ£€æµ‹è¿œç¨‹äº‹ä»¶ï¼ˆå¸¸ç”¨äºåä½œå¼Šé€šä¿¡ï¼‰
local function detectAntiCheatRemotes()
    local suspiciousRemotes = {}
    
    local remoteNames = {
        "AntiCheat", "AC_", "Report", "Flag", "Violation",
        "Cheat", "Hack", "Exploit", "Security", "Kick", "Ban"
    }
    
    for _, obj in ipairs(game:GetDescendants()) do
        if obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction") or obj:IsA("BindableEvent") then
            local name = obj.Name
            for _, remoteName in ipairs(remoteNames) do
                if string.find(name, remoteName) then
                    table.insert(suspiciousRemotes, {
                        Name = name,
                        Path = obj:GetFullName(),
                        Type = obj.ClassName
                    })
                    break
                end
            end
        end
    end
    
    return suspiciousRemotes
end

-- ç»•è¿‡ç®€å•çš„åä½œå¼Šæ£€æµ‹
local function bypassSimpleAntiCheat()
    local bypassSuccess = true
    
    -- å°è¯•ç¦ç”¨å¸¸è§çš„åä½œå¼Šè¿œç¨‹äº‹ä»¶
    local suspiciousRemotes = detectAntiCheatRemotes()
    
    for _, remote in ipairs(suspiciousRemotes) do
        pcall(function()
            -- å°è¯•é‡å†™FireServeræ–¹æ³•
            if remote.Type == "RemoteEvent" then
                local originalFire = getgenv().originalFire or {}
                originalFire[remote.Path] = remote.FireServer
                
                remote.FireServer = function(self, ...)
                    -- é™é»˜å¤„ç†ï¼Œä¸å®é™…å‘é€
                    warn("ğŸš« æ‹¦æˆªåä½œå¼Šè¿œç¨‹äº‹ä»¶: " .. remote.Name)
                    return nil
                end
            end
            
            -- å°è¯•é‡å†™InvokeServeræ–¹æ³•
            if remote.Type == "RemoteFunction" then
                local originalInvoke = getgenv().originalInvoke or {}
                originalInvoke[remote.Path] = remote.InvokeServer
                
                remote.InvokeServer = function(self, ...)
                    -- é™é»˜å¤„ç†ï¼Œä¸å®é™…å‘é€
                    warn("ğŸš« æ‹¦æˆªåä½œå¼Šè¿œç¨‹å‡½æ•°: " .. remote.Name)
                    return nil
                end
            end
        end)
    end
    
    return bypassSuccess
end

-- æ£€æµ‹æ¸¸æˆç¯å¢ƒå®‰å…¨æ£€æŸ¥
local function checkGameSecurity()
    local securityChecks = {}
    
    -- æ£€æŸ¥æ˜¯å¦æœ‰ç¯å¢ƒæ£€æµ‹
    if getgenv().checkenvironment or getgenv().CheckEnvironment then
        table.insert(securityChecks, "ç¯å¢ƒæ£€æµ‹")
    end
    
    -- æ£€æŸ¥æ˜¯å¦æœ‰æ³¨å…¥å™¨æ£€æµ‹
    if getgenv().detectinjector or getgenv().DetectInjector then
        table.insert(securityChecks, "æ³¨å…¥å™¨æ£€æµ‹")
    end
    
    -- æ£€æŸ¥æ˜¯å¦æœ‰é€Ÿåº¦æ£€æµ‹
    if getgenv().checkspeed or getgenv().CheckSpeed then
        table.insert(securityChecks, "é€Ÿåº¦æ£€æµ‹")
    end
    
    -- æ£€æŸ¥æ˜¯å¦æœ‰è„šæœ¬æ‰«æ
    if getgenv().scanscripts or getgenv().ScanScripts then
        table.insert(securityChecks, "è„šæœ¬æ‰«æ")
    end
    
    return securityChecks
end

-- ä¸»åæ¸¸æˆåä½œå¼Šç³»ç»Ÿ
local function startGameAntiCheatBypass()
    if not GAME_ANTICHEAT.ENABLED then return end
    
    -- å»¶è¿Ÿå¯åŠ¨ï¼Œç­‰å¾…æ¸¸æˆå®Œå…¨åŠ è½½
    task.wait(3)
    
    print("ğŸ” å¼€å§‹æ£€æµ‹æ¸¸æˆåä½œå¼Šç³»ç»Ÿ...")
    
    -- æ£€æµ‹åä½œå¼Šç³»ç»Ÿ
    local detectedAC = detectGameAntiCheat()
    local detectedRemotes = detectAntiCheatRemotes()
    local securityChecks = checkGameSecurity()
    
    -- æ˜¾ç¤ºæ£€æµ‹ç»“æœ
    if #detectedAC > 0 or #detectedRemotes > 0 or #securityChecks > 0 then
        local acMessage = "âš ï¸ æ£€æµ‹åˆ°æ¸¸æˆåä½œå¼Šç³»ç»Ÿ:\n"
        
        if #detectedAC > 0 then
            acMessage = acMessage .. "ğŸ“œ åä½œå¼Šè„šæœ¬ (" .. #detectedAC .. " ä¸ª)\n"
            for i, ac in ipairs(detectedAC) do
                if i <= 3 then -- åªæ˜¾ç¤ºå‰3ä¸ª
                    acMessage = acMessage .. "   â€¢ " .. ac.Name .. " (" .. ac.Type .. ")\n"
                end
            end
            if #detectedAC > 3 then
                acMessage = acMessage .. "   â€¢ ... è¿˜æœ‰ " .. (#detectedAC - 3) .. " ä¸ª\n"
            end
        end
        
        if #detectedRemotes > 0 then
            acMessage = acMessage .. "ğŸ“¡ åä½œå¼Šè¿œç¨‹ (" .. #detectedRemotes .. " ä¸ª)\n"
            for i, remote in ipairs(detectedRemotes) do
                if i <= 3 then -- åªæ˜¾ç¤ºå‰3ä¸ª
                    acMessage = acMessage .. "   â€¢ " .. remote.Name .. " (" .. remote.Type .. ")\n"
                end
            end
            if #detectedRemotes > 3 then
                acMessage = acMessage .. "   â€¢ ... è¿˜æœ‰ " .. (#detectedRemotes - 3) .. " ä¸ª\n"
            end
        end
        
        if #securityChecks > 0 then
            acMessage = acMessage .. "ğŸ›¡ï¸ å®‰å…¨æ£€æŸ¥ (" .. #securityChecks .. " ä¸ª)\n"
            for _, check in ipairs(securityChecks) do
                acMessage = acMessage .. "   â€¢ " .. check .. "\n"
            end
        end
        
        -- æ˜¾ç¤ºé€šçŸ¥
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "ğŸ” åä½œå¼Šæ£€æµ‹",
            Text = "æ£€æµ‹åˆ° " .. (#detectedAC + #detectedRemotes + #securityChecks) .. " ä¸ªåä½œå¼Šç»„ä»¶",
            Duration = 8,
            Icon = "rbxassetid://6023426925"
        })
        
        print(acMessage)
        
        -- å°è¯•ç»•è¿‡åä½œå¼Š
        print("ğŸ›¡ï¸ å°è¯•ç»•è¿‡åä½œå¼Šç³»ç»Ÿ...")
        local bypassResult = bypassSimpleAntiCheat()
        
        if bypassResult then
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "âœ… ç»•è¿‡æˆåŠŸ",
                Text = "å·²æˆåŠŸç»•è¿‡éƒ¨åˆ†åä½œå¼Šæ£€æµ‹",
                Duration = 5,
                Icon = "rbxassetid://6023426926"
            })
            print("âœ… åä½œå¼Šç»•è¿‡æªæ–½å·²åº”ç”¨")
        else
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "âš ï¸ ç»•è¿‡å—é™",
                Text = "éƒ¨åˆ†åä½œå¼Šç³»ç»Ÿå¯èƒ½ä»åœ¨è¿è¡Œ",
                Duration = 5,
                Icon = "rbxassetid://6023426925"
            })
            print("âš ï¸ åä½œå¼Šç»•è¿‡å¯èƒ½ä¸å®Œæ•´")
        end
    else
        print("âœ… æœªæ£€æµ‹åˆ°æ˜æ˜¾çš„æ¸¸æˆåä½œå¼Šç³»ç»Ÿ")
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "âœ… ç¯å¢ƒå®‰å…¨",
            Text = "æœªæ£€æµ‹åˆ°æ¸¸æˆåä½œå¼Šç³»ç»Ÿ",
            Duration = 5,
            Icon = "rbxassetid://6023426926"
        })
    end
end

-- ==================== ç¡¬ä»¶IDå’Œæ–‡ä»¶æ“ä½œ ====================

local function generateHardwareID()
    local success, result = pcall(function()
        if isSupported then
            local identifiers = {}
            table.insert(identifiers, tostring(game.Players.LocalPlayer.UserId))
            
            if type(syn) == "table" and syn.crypt then
                local hardwareId = syn.crypt.hash("md5", tostring(tick()) .. "synapse")
                table.insert(identifiers, string.sub(hardwareId, 1, 8))
            end
            
            local randomComponent = HttpService:GenerateGUID(false)
            table.insert(identifiers, string.sub(randomComponent, 1, 8))
            
            return table.concat(identifiers, "_")
        else
            return tostring(game.Players.LocalPlayer.UserId) .. "_" .. tostring(tick())
        end
    end)
    
    return success and result or "fallback_id_" .. tostring(tick())
end

local function safeReadFile(filePath)
    if not isSupported then return nil end
    
    local success, result = pcall(function()
        return readFunc(filePath)
    end)
    
    return success and result or nil
end

local function safeWriteFile(filePath, content)
    if not isSupported then return false end
    
    local success, result = pcall(function()
        writeFunc(filePath, content)
        return true
    end)
    
    return success or false
end

local function getDataPath()
    local basePath = getPathFunc()
    if type(basePath) == "function" then
        basePath = basePath()
    end
    return basePath .. "/" .. KEY_SYSTEM.DATA_FILE
end

-- è®°å¿†ç”¨æˆ·ç³»ç»Ÿ
local function isUserRemembered()
    if not isSupported then return false end
    
    local dataPath = getDataPath()
    local savedData = safeReadFile(dataPath)
    
    if savedData then
        local success, data = pcall(function()
            return HttpService:JSONDecode(savedData)
        end)
        
        if success and data then
            local hardwareID = generateHardwareID()
            if data.hardwareID == hardwareID and data.expireTime and data.expireTime > os.time() then
                return true
            end
        end
    end
    
    return false
end

local function rememberUser()
    if not isSupported then return false end
    
    local dataPath = getDataPath()
    local hardwareID = generateHardwareID()
    local expireTime = os.time() + (KEY_SYSTEM.REMEMBER_DAYS * 24 * 60 * 60)
    
    local data = {
        hardwareID = hardwareID,
        expireTime = expireTime,
        rememberedAt = os.time()
    }
    
    local success = safeWriteFile(dataPath, HttpService:JSONEncode(data))
    return success
end

-- ==================== æ¸æ˜¾ç´«è‰²è¾¹æ¡†æ•ˆæœ ====================

local function createGlowingBorder(parentFrame)
    -- å¤–å‘å…‰è¾¹æ¡†
    local OuterGlow = Instance.new("Frame")
    OuterGlow.Size = UDim2.new(1, 10, 1, 10)
    OuterGlow.Position = UDim2.new(0, -5, 0, -5)
    OuterGlow.BackgroundColor3 = KRNL_COLORS.PRIMARY
    OuterGlow.BackgroundTransparency = 0.7
    OuterGlow.BorderSizePixel = 0
    OuterGlow.ZIndex = 0
    OuterGlow.Parent = parentFrame
    
    local OuterCorner = Instance.new("UICorner")
    OuterCorner.CornerRadius = UDim.new(0, 14)
    OuterCorner.Parent = OuterGlow
    
    -- ä¸­å‘å…‰è¾¹æ¡†
    local MiddleGlow = Instance.new("Frame")
    MiddleGlow.Size = UDim2.new(1, 6, 1, 6)
    MiddleGlow.Position = UDim2.new(0, -3, 0, -3)
    MiddleGlow.BackgroundColor3 = KRNL_COLORS.ACCENT
    MiddleGlow.BackgroundTransparency = 0.5
    MiddleGlow.BorderSizePixel = 0
    MiddleGlow.ZIndex = 0
    MiddleGlow.Parent = parentFrame
    
    local MiddleCorner = Instance.new("UICorner")
    MiddleCorner.CornerRadius = UDim.new(0, 12)
    MiddleCorner.Parent = MiddleGlow
    
    -- å†…å‘å…‰è¾¹æ¡†
    local InnerGlow = Instance.new("Frame")
    InnerGlow.Size = UDim2.new(1, 2, 1, 2)
    InnerGlow.Position = UDim2.new(0, -1, 0, -1)
    InnerGlow.BackgroundColor3 = KRNL_COLORS.SECONDARY
    InnerGlow.BackgroundTransparency = 0.3
    InnerGlow.BorderSizePixel = 0
    InnerGlow.ZIndex = 0
    InnerGlow.Parent = parentFrame
    
    local InnerCorner = Instance.new("UICorner")
    InnerCorner.CornerRadius = UDim.new(0, 11)
    InnerCorner.Parent = InnerGlow
    
    -- åˆ›å»ºè„‰åŠ¨åŠ¨ç”»
    local function createPulseAnimation(frame, delayTime)
        local pulseTween = TweenService:Create(
            frame,
            TweenInfo.new(
                2,
                Enum.EasingStyle.Sine,
                Enum.EasingDirection.InOut,
                -1,
                true,
                delayTime
            ),
            {BackgroundTransparency = frame.BackgroundTransparency + 0.3}
        )
        pulseTween:Play()
    end
    
    createPulseAnimation(OuterGlow, 0)
    createPulseAnimation(MiddleGlow, 0.3)
    createPulseAnimation(InnerGlow, 0.6)
    
    return {OuterGlow, MiddleGlow, InnerGlow}
end

