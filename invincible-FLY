local Translations = {
   --[""] = "",
    ["UNFLY"] = "取消飞行",
    ["Defaultdle"] = "默认",
    ["ChillLevitate"] = "冷静的悬浮",
    ["Stilldle"] = "静止",
    ["FlashyFly"] = "炫目飞",
    ["TargetMark"] = "标靶马克",
    ["PrisonerMark"] = "囚犯马克",
    ["UnsideDown"] = "未下压",
    ["CasualFloat"] = "悠闲浮动",
    ["SheistyMark"] = "蒙面马克",
    ["Preparedldle"] = "准备",
    ["Annoyedldle"] = "恼怒",
    ["Viltrimiteldle"] = "不知道",
    [""] = "",
    [""] = "",
    [""] = "",
    [""] = "",
    [""] = "",
    [""] = "",
    [""] = "",
    [""] = "",
    [""] = "",
